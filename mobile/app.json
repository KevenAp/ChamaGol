{
  "expo": {
    "expo.doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    },
    "scheme": "chamagol",
    "name": "ChamaGol",
    "slug": "ChamaGol",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/apk_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "expo-splash-screen": {
      "backgroundColor": "#FFFFFF",
      "image": "./assets/images/splash-icon.png",
      "dark": {
        "image": "./assets/images/splash-icon-dark.png",
        "backgroundColor": "#000000"
      },
      "imageWidth": 200
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kevenap5.ChamaGol",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/apk_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kevenap5.ChamaGol",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "chamagol"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2a5d08e5-afdc-4785-befc-b54adb5a9941"
      }
    },
    "plugins": ["expo-build-properties"]
  }
}
